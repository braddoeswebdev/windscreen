flash("Navigated to <%= @url %>", "success");
<% 
    m = meta("nowPlaying").set(render :partial => 'control/meta/nav', :locals => {:url => @url, :user => @user})
%>

<% publish_to "/windscreen/ctrl" do %>
    setContent('<%= @url %>');
<% end %>

<% publish_to "/windscreen/meta" do %>
    $("#meta").html('<%= j m.value %>');
    $("#screenControl").attr('src', '<%= noctrl_url %>');
<% end %>



